{% extends "base.twig" %}

{% block content %}
  <div class="content-wrapper">
    <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
      <section>
        <div class="article-body">
          <h1 class="article-h1">{{post.title}}</h1>
          {{post.content}}
        </div>
      </section>
    </article>
  </div><!-- /content-wrapper -->
{% endblock %}

{% extends "base.twig" %}

{% block content %}
  <div class="content-wrapper">
    <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
      <section class="article-content">
        <div class="article-wrapper">
          <div class="article-body">
            <div class="text-center register">
              <img src="{{ site.theme.link }}/static/img/coderdojo-logo-light-bg.png" alt="">
              {{broken_content[0]}}
              {% include 'form.twig' with {'purpose': 'registration'} %}
            </div>
          </div>
        </div>
      </section>
    </article>
  </div><!-- /content-wrapper -->
{% endblock %}

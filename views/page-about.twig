{% extends "base.twig" %}
{% block content %}
<div class="content-wrapper">
  <img src="{{site.theme.link}}/static/img/headers/about.jpg" class="top-banner" />
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    <section class="article-content">
      <div class="article-wrapper">
        <h1 class="article-h1 center">{{post.title}}</h1>
        {{post.content}}
      </div>
    </section>
  </article>
</div>
{% endblock %}
